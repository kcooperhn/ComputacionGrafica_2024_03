<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_reconocimiento_rostros">

    <fragment
        android:id="@+id/nav_reconocimiento_rostros"
        android:name="hn.uth.uthvisionapi.ui.reconocimiento_rostros.ReconocimientoRostrosFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_reconocimiento_rostros" >
        <argument
            android:name="imagen"
            app:argType="android.graphics.Bitmap"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>

    <fragment
        android:id="@+id/nav_etiquetado"
        android:name="hn.uth.uthvisionapi.ui.etiquetado_imagenes.EtiquetadoImagenesFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_etiquetado_imagenes" />

    <fragment
        android:id="@+id/nav_reconocimiento_objetos"
        android:name="hn.uth.uthvisionapi.ui.reconocimiento_codigos_barra.ReconocimientoCodigosBarraFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_reconocimiento_codigos_barra" >
        <argument
            android:name="imagen"
            app:argType="android.graphics.Bitmap"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>

    <fragment
        android:id="@+id/nav_reconocimiento_codigos_barra"
        android:name="hn.uth.uthvisionapi.ui.reconocimiento_codigos_barra.ReconocimientoCodigosBarraFragment"
        android:label="@string/menu_codigos_barra"
        tools:layout="@layout/fragment_reconocimiento_codigos_barra" >
        <argument
            android:name="imagen"
            app:argType="android.graphics.Bitmap"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
</navigation>